<launch>
  <master auto="start"/>
  <param name="/use_sim_time" value="true"/>
  <include file="$(find navigation_stage)/move_base_config/move_base.xml"/>

  <node pkg="stage_ros" type="stageros" name="stageros" args="$(find inc_dude)/maps/intel.world" respawn="false" >
    <param name="base_watchdog_timeout" value="0.2"/>
	<remap from="base_pose_ground_truth" to="ground_truth_odom"/>
	<remap from="odom" to="realistic_odom"/>
  </node>
  
  <node pkg="cg_mrslam" type="sr_slam_map" name="graph_slam" >
  <remap from="odom" to="ground_truth_odom"/>
  </node>
  
  



  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find exp_fraenkel)/launch/exploration_rviz.rviz" />
</launch>
